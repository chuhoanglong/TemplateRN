{
  "name": "DrCare",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "run-p assets:* debugger:install",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ./src",
    "lint:staged": "pretty-quick --staged && lint-staged",
    "clean:android": "cd android && ./gradlew cleanBuildCache && ./gradlew clean && cd ..",
    "clean:ios": "rm -rf ios/build",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "clean": "run-p clean:*",
    "assets": "npx react-native link",
    "assets:icons": "rn-toolbox icons",
    "assets:splashscreen": "rn-toolbox splash",
    "debugger:install": "yarn rndebugger-open",
    "env:dev": "node scripts/set-environment.js development",
    "env:stage": "node scripts/set-environment.js staging",
    "env:prod": "node scripts/set-environment.js production",
    "setup:ios": "cd ios && pod install --repo-update && cd ..",
    "_android": "cd android && ./gradlew clean && cd .. && react-native run-android",
    "android:dev": "run-s env:dev _android",
    "android:stage": "run-s env:stage _android",
    "android:prod": "run-s env:prod _android",
    "_ios": "react-native run-ios --simulator='iPhone 8'",
    "ios:dev": "run-s env:dev _ios",
    "ios:stage": "run-s env:stage _ios",
    "ios:prod": "run-s env:prod _ios",
    "android:bundle:create": "rm -rf android/app/build && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "android:assemble:debug": "cd android && ./gradlew assembleDebug",
    "ios:bundle:create": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "ios:assemble:debug": "xcodebuild -quiet -workspace ios/ReactNativeStarterApp.xcworkspace -scheme ReactNativeStarterApp -configuration Release -sdk iphonesimulator -destination platform='iOS Simulator',OS=13.3,name='iPhone 11 Pro Max' CODE_SIGNING_REQUIRED=NO ONLY_ACTIVE_ARCH=NO",
    "build:dev:android": "run-s env:dev android:bundle:create android:assemble:debug",
    "build:dev:ios": "run-s env:dev setup:ios ios:assemble:debug",
    "dependencies:checkCircular": "madge src/ --circular",
    "dependencies:graph": "madge src/ --circular --image deps-graph.svg",
    "postinstall": "scripts/setup.sh && husky install",
    "import-image": "cd assets && ./bashImportImage.sh && cd .. && cd ..",
    "import-icon": "cd assets && ./bashImportIcon.sh && cd .. && cd ..",
    "generate": "plop",
    "codepush:info-ios": "appcenter codepush ls -k Shadow-Gate/Shadowgate-Wallet-ios",
    "codepush:info-android": "appcenter codepush deployment list -k --app Shadow-Gate/Shadowgate-Wallet-android",
    "codepush-ios:staging": "appcenter codepush release-react -a Shadow-Gate/Shadowgate-Wallet-ios -d Staging",
    "codepush-ios:production": "appcenter codepush promote -a Shadow-Gate/Shadowgate-Wallet-ios -s Staging -d Production",
    "codepush-android:staging": "appcenter codepush release-react -a Shadow-Gate/Shadowgate-Wallet-android -d Staging",
    "codepush-android:production": "appcenter codepush promote -a Shadow-Gate/Shadowgate-Wallet-android -s Staging -d Production",
    "deploy-init:ios": "cd ios && fastlane init && cd .. && cd ..",
    "deploy-init:android": "cd android && fastlane init && cd .. && cd ..",
    "deploy:ios": "cd ios && fastlane ios distribution && cd ..",
    "deploy:android": "cd android && fastlane android distribution && cd .."
  },
  "dependencies": {
    "@eva-design/eva": "^2.1.0",
    "@nartc/react-native-barcode-mask": "^1.2.0",
    "@react-native-community/async-storage": "1.12.1",
    "@react-native-community/blur": "^3.6.0",
    "@react-native-community/hooks": "^2.6.0",
    "@react-native-community/masked-view": "0.1.11",
    "@react-native-firebase/app": "^12.1.0",
    "@react-native-firebase/messaging": "^12.1.0",
    "@react-native-google-signin/google-signin": "^7.0.4",
    "@react-navigation/bottom-tabs": "^5.11.11",
    "@react-navigation/drawer": "^5.12.5",
    "@react-navigation/native": "5.9.4",
    "@react-navigation/stack": "5.14.5",
    "@reduxjs/toolkit": "^1.5.1",
    "@sentry/react-native": "^3.2.3",
    "@types/lodash": "^4.14.170",
    "@ui-kitten/components": "^5.1.0",
    "appcenter": "^4.2.0",
    "appcenter-analytics": "^4.2.0",
    "appcenter-crashes": "^4.2.0",
    "i18next": "20.3.1",
    "lodash": "^4.17.21",
    "react": "17.0.2",
    "react-countdown-circle-timer": "^2.5.4",
    "react-i18next": "^11.10.0",
    "react-native": "0.64.2",
    "react-native-autoheight-webview": "^1.5.8",
    "react-native-bottomsheet-reanimated": "^0.0.44",
    "react-native-camera": "^3.42.2",
    "react-native-code-push": "^7.0.1",
    "react-native-config": "^1.4.3",
    "react-native-countdown-component": "^2.7.1",
    "react-native-customisable-alert": "^0.1.20",
    "react-native-fast-image": "^8.3.4",
    "react-native-fbsdk": "^2.0.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-interactable-reanimated": "^0.0.15",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localize": "2.1.1",
    "react-native-parsed-text": "^0.0.22",
    "react-native-qrcode-svg": "^6.1.1",
    "react-native-reanimated": "2.2.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.3.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^12.1.1",
    "react-native-vector-icons": "^8.1.0",
    "react-native-version-number": "^0.3.6",
    "react-native-webview": "^11.6.5",
    "react-redux": "^7.2.4",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "typescript": "4.3.2",
    "with-query": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@babel/runtime": "7.14.0",
    "@panz3r/react-native-toolbox": "^2.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@react-native-community/eslint-plugin": "^1.1.0",
    "@types/color": "^3.0.2",
    "@types/jest": "^26.0.23",
    "@types/node": "15.12.1",
    "@types/react": "17.0.9",
    "@types/react-native": "0.64.10",
    "@types/react-native-countdown-component": "^2.7.0",
    "@types/react-native-fbsdk": "^3.0.1",
    "@types/react-native-snap-carousel": "^3.8.5",
    "@types/react-native-vector-icons": "^6.4.7",
    "@types/react-redux": "^7.1.16",
    "@types/react-test-renderer": "^17.0.1",
    "@types/redux-logger": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "babel-jest": "27.0.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "lint-staged": "11.0.0",
    "madge": "^4.0.2",
    "metro-react-native-babel-preset": "^0.66.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.3.1",
    "pretty-quick": "^3.1.0",
    "react-native-debugger-open": "^0.3.25",
    "react-test-renderer": "17.0.2",
    "remote-redux-devtools": "^0.5.16",
    "shx": "^0.3.3",
    "ts-jest": "26.5.6"
  },
  "jest": {
    "preset": "react-native"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix"
  },
  "madge": {
    "tsConfig": "./tsconfig.json",
    "fileExtensions": "ts",
    "excludeRegExp": [
      ".*\\.test\\.ts$",
      ".*\\.test\\.tsx$"
    ],
    "detectiveOptions": {
      "ts": {
        "skipTypeImports": true
      }
    }
  }
}
